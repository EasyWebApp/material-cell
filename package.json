{
    "name": "material-cell",
    "version": "3.0.0-rc.0",
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "description": "Material Web Components's JSX Type definition for WebCell engine",
    "keywords": [
        "web",
        "component",
        "typescript",
        "jsx",
        "webcell",
        "material"
    ],
    "homepage": "https://material.web-cell.dev/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EasyWebApp/material-cell.git"
    },
    "bugs": {
        "url": "https://github.com/EasyWebApp/material-cell/issues"
    },
    "types": "source/index.d.ts",
    "dependencies": {
        "dom-renderer": "^2.1.3",
        "web-cell": "^3.0.0-rc.14",
        "web-utility": "^4.1.3"
    },
    "peerDependencies": {
        "@material/web": "^1.2.0"
    },
    "devDependencies": {
        "@material/web": "^1.2.0",
        "husky": "^9.0.10",
        "lint-staged": "^15.2.2",
        "parcel": "~2.11.0",
        "prettier": "^3.2.5",
        "typescript": "~5.3.3"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{html,md,json,ts,tsx}": "prettier --write"
    },
    "scripts": {
        "prepare": "husky",
        "test": "lint-staged",
        "clean": "rm -rf .parcel-cache/ test/dist/",
        "start": "npm run clean  &&  cd test/  &&  parcel --open",
        "build": "npm run clean  &&  cd test/  &&  parcel build --public-url ."
    }
}
