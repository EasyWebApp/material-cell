{
  "name": "material-cell",
  "version": "0.9.1",
  "description": "Material-design Web component library based on WebCell",
  "keywords": [
    "material",
    "design",
    "web",
    "component",
    "web-cell"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://web-cell-ht.ml/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/material-cell.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/material-cell/issues"
  },
  "browser": "dist/index.js",
  "directories": {
    "lib": "source/",
    "test": "test/"
  },
  "scripts": {
    "lint": "eslint source/ test/ --fix",
    "build": "npm run lint  &&  web-cell pack",
    "dev": "web-cell preview",
    "test": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test  &&  git add ."
    }
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "web-cell": "^0.10.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "resize-observer-polyfill": "^1.5.0",
    "web-cell-cli": "^0.7.5",
    "whatwg-fetch": "^3.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ]
    ]
  }
}
