{
    "compilerOptions": {
        "rootDir": "source/",
        "target": "ES5",
        "module": "ESNext",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "jsxFactory": "createCell",
        "lib": ["ES2019", "DOM"]
    },
    "typedocOptions": {
        "name": "Material Cell",
        "mode": "file",
        "excludeExternals": true,
        "excludeNotExported": true,
        "readme": "./README.md",
        "out": "docs/"
    }
}
